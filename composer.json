{
    "name": "peoplecloudhrm/peoplecloud-php",
    "description": "PeopleCloud PHP Library",
    "type": "library",
    "require": {
        "php": "7.0.*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Aguilera",
            "email": "a.carlosing@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PeopleCloud\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeopleCloud\\": [
                "tests/",
                "tests/PeopleCloud/"
            ]
        }
    }
}
